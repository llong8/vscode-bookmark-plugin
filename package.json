{
  "name": "vscode-bookmark-plugin",
  "displayName": "Bookmark Manager",
  "description": "一个简单易用的代码书签插件，可按文件夹分类、文件夹嵌套、书签和文件夹任意拖放。",
  "version": "1.0.8",
  "publisher": "llong",
  "icon": "resources/logo2.png",
  "keywords": [
    "bookmark",
    "bookmark-manager",
    "bookmarks",
    "jump",
    "keybindings",
    "markmulti-root-ready",
    "navigation",
    "selection",
    "sticky"
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/llong8/vscode-bookmark-plugin.git"
  },
  "homepage": "https://github.com/llong8/vscode-bookmark-plugin/blob/main/README.md",
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "bookmark.addBookmark",
        "title": "%bookmark.addBookmark%",
        "icon": "$(bookmark)"
      },
      {
        "command": "bookmark.goToBookmark",
        "title": "Go to Bookmark"
      },
      {
        "command": "bookmark.removeBookmark",
        "title": "%bookmark.removeBookmark%"
      },
      {
        "command": "bookmark.renameBookmark",
        "title": "%bookmark.renameBookmark%"
      },
      {
        "command": "bookmark.createFolder",
        "title": "%bookmark.createFolder%",
        "icon": "$(new-folder)"
      },
      {
        "command": "bookmark.renameFolder",
        "title": "%bookmark.renameFolder%"
      },
      {
        "command": "bookmark.deleteFolder",
        "title": "%bookmark.deleteFolder%"
      },
      {
        "command": "bookmark.refresh",
        "title": "%bookmark.refresh%",
        "icon": "$(refresh)"
      },
      {
        "command": "bookmark.exportBookmarks",
        "title": "%bookmark.exportBookmarks%",
        "icon": "$(export)"
      },
      {
        "command": "bookmark.importBookmarks",
        "title": "%bookmark.importBookmarks%",
        "icon": "$(import)"
      },
      {
        "command": "bookmark.createSubFolder",
        "title": "%bookmark.createSubFolder%",
        "icon": "$(new-folder)"
      }
    ],
    "keybindings": [
      {
        "command": "bookmark.addBookmark",
        "key": "ctrl+shift+b",
        "mac": "cmd+shift+b",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "bookmark.addBookmark",
          "group": "navigation",
          "when": "editorTextFocus"
        }
      ],
      "view/title": [
        {
          "command": "bookmark.addBookmark",
          "when": "view == bookmarkView",
          "group": "navigation@1"
        },
        {
          "command": "bookmark.createFolder",
          "when": "view == bookmarkView",
          "group": "navigation@2"
        },
        {
          "command": "bookmark.refresh",
          "when": "view == bookmarkView",
          "group": "navigation@3"
        },
        {
          "command": "bookmark.exportBookmarks",
          "when": "view == bookmarkView",
          "group": "navigation@4"
        },
        {
          "command": "bookmark.importBookmarks",
          "when": "view == bookmarkView",
          "group": "navigation@5"
        }
      ],
      "view/item/context": [
        {
          "command": "bookmark.renameBookmark",
          "when": "view == bookmarkView && viewItem == bookmark",
          "group": "1_modification"
        },
        {
          "command": "bookmark.removeBookmark",
          "when": "view == bookmarkView && viewItem == bookmark",
          "group": "2_removal"
        },
        {
          "command": "bookmark.renameFolder",
          "when": "view == bookmarkView && viewItem == folder",
          "group": "1_modification"
        },
        {
          "command": "bookmark.createSubFolder",
          "when": "view == bookmarkView && viewItem == folder",
          "group": "1_modification"
        },
        {
          "command": "bookmark.deleteFolder",
          "when": "view == bookmarkView && viewItem == folder",
          "group": "2_removal"
        }
      ]
    },
    "views": {
      "bookmark": [
        {
          "id": "bookmarkView",
          "name": "%bookmark.view.name%",
          "icon": "$(bookmark)",
          "contextualTitle": "%bookmark.view.name%"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "bookmark",
          "title": "%bookmark.view.name%",
          "icon": "resources/bookmark.svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "^22.15.25",
    "typescript": "^4.9.4"
  }
}
